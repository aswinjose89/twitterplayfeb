<!DOCTYPE html>
<html lang="en">
   <head>
      <!-- Meta -->
      <meta charset="utf-8" />
      <meta http-equiv="X-UA-Compatible" content="IE=edge" />
      <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" />
      <!-- Document title -->
      <title>TweetZoom 1.0</title>
      <meta name="description" content="AppUI - Admin Dashboard Template & UI Framework" />
      <meta name="author" content="rustheme" />
      <meta name="robots" content="noindex, nofollow" />
      <!-- Favicons -->
      <link rel="apple-touch-icon" href="/static/login/img/favicons/apple-touch-icon.png" />
      <link rel="icon" href="/static/login/img/favicons/favicon.ico" />
      <!-- Bootstrap Core CSS -->
      <link href="/static/login/css/bootstrap.min1.css" rel="stylesheet">
      <!-- Custom CSS -->
      <link href="/static/login/css/grayscale.css" rel="stylesheet">
      <!-- Pricing Table CSS -->
      <link href="/static/css/pricing-table.css" rel="stylesheet">

      <link href="/static/css/main.css" rel="stylesheet">

      <!-- Custom Fonts -->
      <link href="/static/login/font-awesome/css/font-awesome.min.css" rel="stylesheet" type="text/css">
      <link href="http://fonts.googleapis.com/css?family=Lora:400,700,400italic,700italic" rel="stylesheet" type="text/css">
      <link href="http://fonts.googleapis.com/css?family=Montserrat:400,700" rel="stylesheet" type="text/css">
   </head>
   <style>
      html, body {margin: 0; height: 100%;}
   </style>
   <script>
     var appSettings = <%- appSettings %>;
   </script>
   <body id="page-top" class="bg1">
      <!-- Main Start -->
      <main class="site-content">
        <div id='stars'></div>
        <div id='stars2'></div>
        <div id='stars3'></div>
         <!-- Pricing Style2 (BG Image) Start -->
         <section class="sec-spacer bg2">
            <div class="container">
               <div class="spacer3"></div>
               <div class="row pricing-style2">
                  <div class="col-md-2"></div>
                  <div class="col-md-4 col-sm-6">
                     <div class="pricing-plan">
                        <div class="pricing-head">
                           <div class="icon">
                              <i class="fa fa-briefcase"></i>
                           </div>
                           <img src="../static/img/shape_blue.svg" class="shape" alt="">
                           <div class="name">
                              Basic <br>PLAN
                           </div>
                           <div class="pricing-footer">
                              <a href="#" onclick="proceedBasicPricing(event)" class="p-button">PROCEED</a>
                           </div>
                           <div class="short-desc">
                              A powerful, real-time Social Analytics Platform
                           </div>
                           <div class="price">
                              <span class="value">
                              $0
                              </span>
                              <span class="duration">
                              / monthly
                              </span>
                           </div>
                        </div>
                        <div class="pricing-body">
                           <ul>
                              <li>Limited to 50k Tweets<i class="fa fa-check"></i></li>
                              <li>100 GB Space <i class="fa fa-check"></i></li>
                              <li>1 Domain Name <i class="fa fa-close"></i></li>
                              <li>500GB Bandwidth <i class="fa fa-check"></i></li>
                              <li>25 Mysql Databases <i class="fa fa-check"></i></li>
                              <li>Enhanced Security <i class="fa fa-close"></i></li>
                           </ul>
                        </div>
                        <div class="pricing-footer">
                           <a href="#" onclick="proceedBasicPricing(event)" class="p-button">PROCEED</a>
                        </div>
                     </div>
                  </div>
                  <div class="col-md-4 col-sm-6 hidden-sm hidden-xs">
                     <div class="pricing-plan">
                        <div class="pricing-head">
                           <div class="icon">
                              <i class="fa fa-rocket"></i>
                           </div>
                           <img src="../static/img/shape_blue.svg" class="shape" alt="">
                           <div class="name">
                              Enterprise <br>PLAN
                           </div>
                           <div class="pricing-footer">
                              <a href="profile_reg" class="p-button">PROCEED</a>
                           </div>
                           <div class="short-desc">
                              A powerful, real-time Social Analytics Platform
                           </div>
                           <div class="price">
                              <span class="value">
                              <%= amount%>
                              </span>
                              <span class="duration">
                              / monthly
                              </span>
                           </div>
                        </div>
                        <div class="pricing-body">
                           <ul>
                              <li>Unlimited Tweets <i class="fa fa-check"></i></li>
                              <li>100 GB Space <i class="fa fa-check"></i></li>
                              <li>1 Domain Name <i class="fa fa-close"></i></li>
                              <li>500GB Bandwidth <i class="fa fa-check"></i></li>
                              <li>25 Mysql Databases <i class="fa fa-check"></i></li>
                              <li>Enhanced Security <i class="fa fa-close"></i></li>
                           </ul>
                        </div>
                        <div class="pricing-footer">
                           <a href="profile_reg" class="p-button">PROCEED</a>
                        </div>
                     </div>
                  </div>
                  <div class="col-md-2"></div>
               </div>
            </div>
         </section>
         <!-- Pricing Style2 (BG Image) End -->
      </main>
      <!-- Main End -->
      <!-- jQuery -->
      <script src="/static/socket.io/socket.io-1.3.5.js"></script>
      <script src="/static/login/js/jquery.js"></script>
      <!-- Bootstrap Core JavaScript -->
      <script src="/static/login/js/bootstrap.min.js"></script>
      <script src="/static/js/app/pricing.js"></script>
   </body>
</html>
